{
    "if": {
        "allOf": [{
                "field": "type",
                "equals": "Microsoft.Compute/virtualMachines"
            },
            {
                "not": {
                    "field": "Microsoft.Network/virtualNetworks.name",
                    "equals": "parameters('VNET_NAME')"
                }
            }
        ]
    },
    "then": {
        "effect": "deny"
    }
}